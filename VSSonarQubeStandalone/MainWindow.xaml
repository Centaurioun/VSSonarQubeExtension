<Window x:Class="VSSonarQubeStandalone.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewModel="clr-namespace:VSSonarExtensionUi.ViewModel;assembly=VSSonarExtensionUi"
        xmlns:view="clr-namespace:VSSonarExtensionUi.View;assembly=VSSonarExtensionUi"
        Title="MainWindow" Height="600" Width="800">
    <Window.Resources>
        <DataTemplate DataType="{x:Type viewModel:SonarQubeViewModel}" x:Key="DefaultView">
            <view:SonarQubeUserControl DataContext="{Binding}"/>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <ContentControl ContentTemplate="{StaticResource DefaultView}"/>
    </Grid>

</Window>
