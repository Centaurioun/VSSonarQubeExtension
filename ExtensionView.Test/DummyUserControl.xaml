<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExtensionView.Test" x:Class="ExtensionView.Test.DummyUserControl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<UserControl.Resources>
		<local:DummyOptionsController x:Key="DummyOptionsControllerDataSource" d:IsDataSource="True"/>
	</UserControl.Resources>
	<UserControl.DataContext>
		<Binding Source="{StaticResource DummyOptionsControllerDataSource}"/>
	</UserControl.DataContext>
	<Grid>
		<Button Content="Button" HorizontalAlignment="Left" Margin="60,146.149,0,0" VerticalAlignment="Top" Width="175" Height="42.851"/>
		<TextBox HorizontalAlignment="Left" Height="32.836" Margin="60,34.328,0,0" TextWrapping="Wrap" Text="{Binding TextBox}" VerticalAlignment="Top" Width="175"/>

	</Grid>
</UserControl>
